//Hex to Decimal
function hexToDec(hexString){
  return parseInt(hexString,16)
}
//Drink about
function peopleWithAgeDrink(old) {
  if(old < 14){
   return 'drink toddy'
 }
else if(old < 18){
   return 'drink coke'
 }
 else if(old < 21){
   return 'drink beer'
 }
  else{
   return 'drink whisky'
 }
};
