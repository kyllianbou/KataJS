  //Training JS #9: loop statement --while and do..while
  function padIt(str,n){
    while (n>0){
      if (n%2 === 0){
        str = str + "*";
      }else{
        str = "*" + str ;
      }n --;
    }return str
  }
  //Find Nearest square number
  function nearestSq(n){
    return Math.round(n**0.5)**2
  }
